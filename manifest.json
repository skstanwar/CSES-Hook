{
  "manifest_version": 3,
  "name": "CSES Hook",
  "version": "1.0",
  "description": "Open IDE in Web to write the code on CSES in real time, no need to upload files.",
  "author": "Sanjay Saini",
  "homepage_url": "https://github.com/skstanwar/CSES-Hook",
  "license": "MIT",
  "permissions": ["scripting", "activeTab"],
  "content_scripts": [
    {
      "matches": ["https://cses.fi/problemset/task/*"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["src/*.txt"],
      "matches": ["<all_urls>"]
    }
  ],
  "host_permissions": [
    "https://cses.fi/*"
  ]
}
